<h1>Teachers</h1>

<% @teachings.each do |teaching| %>
	<div class="teacher">
		<h4>
			<%= link_to teaching.teacher.name, teaching_path(teaching) %>
		</h4>
		<p>
			From: <%= teaching.country_of_origin %> <br>
			Teaches: <%= teaching.subject_to_teach %> <br>
			Education level: <%= teaching.education %> <br>
			$<%= teaching.price %>
			<%= teaching.price_currency %>
		</p>
	</div>
	
<% end %>
